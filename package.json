{
    "name": "naga-ts",
    "version": "0.0.1",
    "repository": {
        "type": "github",
        "url": "https://github.com/gongzhaohui/NAGA-TS"
    },
    "license": "MIT",
    "scripts": {
        "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
        "serve:ssr": "node dist/.bin/server.bundle.js",
        "build:client-and-server-bundles": "ng build --prod && ng run naga-ts:server",
        "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
        "ng": "ng",
        "start:ng": "ng serve",
        "build": "ng build",
        "test:ng": "ng test",
        "lint:ng": "ng lint",
        "test:e2e:ng": "ng e2e",
        "format": "prettier --write \"src/**/*.ts\"",
        "start": "ts-node -r tsconfig-paths/register src/server/main.ts",
        "start:dev": "nodemon -V",
        "start:debug": "nodemon --config nodemon-debug.json",
        "prestart:prod": "rimraf dist/.bin && tsc",
        "start:prod": "node dist/.bin/server.bundle.js",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@angular/animations": "^10.0.5",
        "@angular/common": "^10.0.5",
        "@angular/compiler": "^10.0.5",
        "@angular/core": "^10.0.5",
        "@angular/forms": "^10.0.5",
        "@angular/http": "^7.2.16",
        "@angular/platform-browser": "^10.0.5",
        "@angular/platform-browser-dynamic": "^10.0.5",
        "@angular/platform-server": "^10.0.5",
        "@angular/router": "^10.0.5",
        "@nestjs/common": "^7.3.2",
        "@nestjs/core": "^7.3.2",
        "@nestjs/graphql": "^7.5.5",
        "@nestjs/jwt": "^7.1.0",
        "@nestjs/passport": "^7.1.0",
        "@nestjs/platform-express": "^7.3.2",
        "@nestjs/swagger": "^4.5.12",
        "@nestjs/typeorm": "^7.1.0",
        "@nguniversal/express-engine": "^10.0.1",
        "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
        "apollo-server-express": "^2.16.1",
        "arangojs": "^6.14.1",
        "bcryptjs": "^2.4.3",
        "class-validator": "^0.12.2",
        "cookie-parser": "^1.4.5",
        "core-js": "^3.6.5",
        "dotenv": "^8.2.0",
        "graphql": "^15.3.0",
        "graphql-tools": "^6.0.15",
        "helmet": "^3.23.3",
        "joi": "^17.1.1",
        "lodash": "^4.17.19",
        "msnodesqlv8": "^2.0.0",
        "mssql": "^6.2.1",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.6.0",
        "rxjs-compat": "^6.6.0",
        "swagger-ui-express": "^4.1.4",
        "typeorm": "^0.2.25",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1000.4",
        "@angular/cli": "^10.0.4",
        "@angular/compiler-cli": "^10.0.5",
        "@angular/language-service": "^10.0.5",
        "@nestjs/testing": "^7.3.2",
        "@types/bcryptjs": "^2.4.2",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.7",
        "@types/helmet": "0.0.47",
        "@types/jasmine": "~3.5.11",
        "@types/jasminewd2": "~2.0.8",
        "@types/jest": "^26.0.7",
        "@types/joi": "^14.3.4",
        "@types/lodash": "^4.14.158",
        "@types/mssql": "^6.0.3",
        "@types/node": "^14.0.26",
        "@types/passport-http-bearer": "^1.0.35",
        "@types/passport-local": "^1.0.33",
        "@types/passport-jwt": "^3.0.3",
        "@types/request": "^2.48.5",
        "@types/supertest": "^2.0.10",
        "circular-json": "^0.5.9",
        "codelyzer": "~6.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.2",
        "jest": "^26.1.0",
        "karma": "~5.1.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~3.3.1",
        "karma-jasmine-html-reporter": "^1.5.4",
        "nodemon": "^2.0.4",
        "nodemon-webpack-plugin": "^4.3.2",
        "prettier": "^2.0.5",
        "protractor": "~7.0.0",
        "rimraf": "^3.0.2",
        "supertest": "^4.0.2",
        "ts-jest": "^26.1.3",
        "ts-loader": "^8.0.1",
        "ts-node": "^8.10.2",
        "tsconfig-paths": "^3.9.0",
        "tslint": "6.1.2",
        "tslint-loader": "^3.6.0",
        "typescript": "~3.9.7",
        "webpack": "^4.44.0",
        "webpack-cli": "^3.3.12",
        "webpack-node-externals": "^2.5.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src/server",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
